{
    "name": "modxcms/teleport",
    "description": "A Packaging Toolkit for MODX Revolution",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Coward",
            "email": "jason@opengeek.com",
            "homepage": "http://www.opengeek.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "aws/aws-sdk-php": "2.4.*",
        "react/react": "dev-master"
    },
    "require-dev": {
        "symfony/finder": "~2.2",
        "symfony/process": "~2.1@dev",
        "symfony/filesystem": "~2.4"
    },
    "suggest": {
        "ext-libevent": "It is critical to have this or ext-ev if you plan to use the server or endpoint listener.",
        "ext-ev": "It is critical to have this or ext-libevent if you plan to use the server or endpoint listener."
    },
    "bin": [
        "bin/compile",
        "bin/compile.php",
        "bin/endpoint",
        "bin/endpoint.php",
        "bin/server",
        "bin/server.php",
        "bin/teleport",
        "bin/teleport.php"
    ],
    "autoload": {
        "psr-0": {
            "Teleport": "src/"
        }
    }
}
